---
import favIcon from '~/application/shared/assets/favicons/favicon.ico';
import favIconSvg from '~/application/shared/assets/favicons/favicon.svg';
import appleTouchIcon from '~/application/shared/assets/favicons/apple-touch-icon.png';
---

<link rel="shortcut icon" href={favIcon} />
<link rel="icon" type="image/svg+xml" href={favIconSvg.src} />
<link rel="mask-icon" href={favIconSvg.src} color="#8D46E7" />
<link rel="apple-touch-icon" sizes="180x180" href={appleTouchIcon.src} />
